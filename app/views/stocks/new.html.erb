<h1>New Stock</h1>

<%= form_for @stock do |f| %>
    <div class="field">
        <%= f.label :stock %><br />
        <%= f.text_field :stock %>
    </div>

    <div class="field">
        <%= f.label :date %><br />
        <%= f.text_field :date %>
    </div>

    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>

