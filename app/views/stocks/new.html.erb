<h1>New Stock</h1>

<%= form_for @stock do |f| %>
    <div class="field">
        <%= f.label :symbol %><br />
        <%= f.text_field :symbol %>
    </div>

    <div class="field">
        <%= f.label :date %><br />
        <%= f.date_field :date %>
    </div>

    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>

<%= link_to 'Back', root_path%>